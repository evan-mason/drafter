@model IEnumerable<Pick> // This is the same as a List, I could use this too
<h1>Picks</h1>
<p>Remaining:@Model.Count()</p>
<div class="row">
    @foreach (var p in Model)
    {
        <div class="col-md-1">
            <div class="border bg-light rounded p-1">
                <!-- <img src="~/img/p.ArtId.jpg" class="image-fliud" /> -->
                <h3>@p.PickNumber - @p.FantasyTeam.Name</h3>
            </div>
        </div>
    }
</div>